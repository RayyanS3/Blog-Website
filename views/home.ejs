<%- include("partials/header") -%>
<div class="content">
  <h1>Home</h1>
  <p><%= homeText %></p>
  <% posts.forEach(function(post) { %>
  <h1><%= post.title; %></h1>
  <% if(post.content.length > 100) { %>
  <p>
    <%= post.content.substring(0,100) + "..."; %><a
      href="/posts/<%=post.title%>"
      >Read more</a
    >
  </p>
  <% } %> <% }); %>
</div>
<%- include("partials/footer") -%>
